# -*- mode: snippet -*-
# name: personal cpp lowbound function
# key: __lowbound
# group: file templates
# contributor: name
# --

int lowI(vector<int> &nums, int t) {
  int l = 0, r = nums.size() - 1;
  while (l < r) {
    int m = (l + r) / 2;
    if (nums[m] >= t)
      r = m;
    else
      l = m + 1;
  }
  return l;
}

